<?xml version="1.0" encoding="utf-8"?>
<resources>
    <!--My 탭 텍스트 설정-->
    <style name="MyTabTextStyle" parent="TextAppearance.Design.Tab">
        <item name="android:textSize">16sp</item>
        <item name="android:textStyle">bold</item>
    </style>

    <!--Bottomnavigation 글자 설정-->
    <style name="BottomNavigationView" parent="@style/TextAppearance.AppCompat.Caption">
        <item name="android:textSize">10sp</item>
        <item name="android:fontFamily">@font/pretendard_black</item>
    </style>
    <style name="BottomNavigationView.Active" parent="@style/TextAppearance.AppCompat.Caption">
        <item name="android:textSize">10sp</item>
    </style>

    <!--ratingbar custom-->
    <style name="CustomRatingBar" parent="Widget.AppCompat.RatingBar">
        <item name="android:progressDrawable">@drawable/custom_ratingbar</item>
    </style>

</resources>